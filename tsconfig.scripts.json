{
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["../src/*"]
    },
    "noImplicitAny": true,
    "sourceMap": false,
    "baseUrl": ".",
    "noEmit": false
  },
  "include": ["./scripts/*.ts", "./src/app/play/[id]/sharedUtils.ts"]
}
